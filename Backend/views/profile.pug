extends layout 

block navHeader 
    include includes/navHeader.pug

block content 
    div(class='header')
        h3="It's your profile"
    div(class='edit-profile-btn')
        input(class='profile-button' id='edit-profile' type='submit' value='Edit Profile')
        
    div(class='content')
        h2='Liked Libs'
        table(cellpadding='0' cellspacing='0' class='profile-table')
            thead 
                tr
                    th Name 
                    th Creator 
        div(class='table-content')
            table(cellpadding='0' cellspacing='0' border='0')
                tbody 
                    tr 
                        td Uncle Lester's outhouse 
                        td Mad-Liberace
                    
    div(class='content')
        p This is where you can see your saved Mad-libs
   
        
    